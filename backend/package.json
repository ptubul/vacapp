{
   "name": "travel_easily",
   "version": "1.0.0",
   "description": "backend",
   "main": "app.js",
   "scripts": {
      "devv": "nodemon --exec ts-node ./src/server.ts",
      "build": "tsc",
      "test": "cross-env NODE_ENV=test tsc && jest --detectOpenHandles --coverage --runInBand  --forceExit",
      "testAuth": "cross-env NODE_ENV=test tsc && jest --detectOpenHandles   --runInBand  ./src/test/auth.test.ts",
      "testTrip": " tsc && jest   --runInBand ./src/test/trips.test.ts",
      "testFile": "cross-env NODE_ENV=test tsc && jest --detectOpenHandles --coverage --runInBand  ./src/test/file.test.ts",
      "testUser": "cross-env NODE_ENV=test tsc && jest --detectOpenHandles  --runInBand  ./src/test/user.test.ts",
      "start": "ts-node src/index.ts",
      "prod": "cp .envprod .env && tsc -p tsconfig_prod.json && pm2 restart ecosystem.config.js --env production",
      "prod1": "cp .envprod .env && tsc -p tsconfig_prod.json && node ./dist/src/server.js",
      "typeorm": "typeorm-ts-node-commonjs"
   },
   "author": "David Erenfeld",
   "license": "ISC",
   "dependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.1",
      "@types/jsonwebtoken": "^9.0.5",
      "@types/multer": "^1.4.11",
      "@types/supertest": "^6.0.2",
      "@types/swagger-jsdoc": "^6.0.4",
      "@types/swagger-ui-express": "^4.1.6",
      "@typescript-eslint/eslint-plugin": "^6.16.0",
      "@typescript-eslint/parser": "^6.16.0",
      "bcrypt": "^5.1.1",
      "body-parser": "^1.20.2",
      "cors": "^2.8.5",
      "cross-env": "^7.0.3",
      "dotenv": "^16.4.5",
      "eslint": "^8.56.0",
      "express": "^4.19.2",
      "google-auth-library": "^9.6.3",
      "install": "^0.13.0",
      "jsonwebtoken": "^9.0.2",
      "mongoose": "^8.0.3",
      "multer": "^1.4.5-lts.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^5.0.0",
      "ts-node": "^10.9.2",
      "typeorm": "0.3.20",
      "uuid": "^10.0.0"
   },
   "devDependencies": {
      "@types/jest": "^29.5.11",
      "@types/node": "^20.12.12",
      "@types/supertest": "^6.0.2",
      "jest": "^29.7.0",
      "nodemon": "^3.1.0",
      "supertest": "^6.3.3",
      "ts-jest": "^29.1.1",
      "ts-node": "10.9.1",
      "typescript": "^5.4.5"
   }
}
